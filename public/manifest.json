{
	"name": "Tabs Manager",
	"description": "Tool to save, manage and open groups of tabs",
	"version": "1.0",
	"manifest_version": 3,
	"action": {
		"default_popup": "index.html",
		"default_title": "Tabs Manager"
	},
	"icons": {
		"16": "bookmark-tabs.png",
		"48": "bookmark-tabs.png",
		"128": "bookmark-tabs.png"
	},
	"permissions": ["storage", "tabs"]
}
